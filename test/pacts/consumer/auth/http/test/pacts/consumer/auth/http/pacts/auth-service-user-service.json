{
  "consumer": {
    "name": "auth-service"
  },
  "interactions": [
    {
      "description": "a request to verify valid user credentials",
      "providerState": "a user exists with this email and password",
      "request": {
        "body": {
          "email": "user@example.com",
          "password": "super-secret-password"
        },
        "headers": {
          "Accept": "application/json",
          "Content-Type": "application/json"
        },
        "matchingRules": {
          "$.body.email": {
            "match": "type"
          },
          "$.body.password": {
            "match": "type"
          }
        },
        "method": "POST",
        "path": "/internal/users/verify"
      },
      "response": {
        "body": {
          "email": "user@example.com",
          "id": "8a26b19d-8a33-4ece-87b1-7b7c2fb9e0ad",
          "status": "active"
        },
        "headers": {
          "Content-Type": "application/json; charset=utf-8"
        },
        "matchingRules": {
          "$.body.email": {
            "match": "type"
          },
          "$.body.id": {
            "match": "type"
          },
          "$.body.status": {
            "match": "type"
          }
        },
        "status": 200
      }
    }
  ],
  "metadata": {
    "pact-go": {
      "version": "2.4.2"
    },
    "pactRust": {
      "ffi": "0.4.28",
      "mockserver": "1.2.16",
      "models": "1.3.5"
    },
    "pactSpecification": {
      "version": "2.0.0"
    }
  },
  "provider": {
    "name": "user-service"
  }
}